
@font-face {
	font-family: 'iconfont';
	src: url(data:font/ttf;base64,) format('truetype');
}
.iconfont {
	font-family: "iconfont";
	font-size: 15px;
	color: #BBBBBB;
	font-weight: bold;
	font-style: normal;
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}
.arrowView:before {
	content: "\e64a";
}
.adBaseView {
	width: 100%;
	background-color: #FFFFFF;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	padding-top: 30rpx;
}
.baseViewHover {
	background-color: #F3F3F3;
}
.adRowView {
	width: calc(100% - 48rpx);
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.headView {
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
	text-align: left;
	margin-left: 30rpx;
	font-size: 28rpx;
	/*超出一行点点点*/
	overflow: hidden;
	/*超出的部分隐藏起来。*/
	white-space: nowrap;
	/*不显示的地方用省略号...代替*/
	text-overflow: ellipsis;
	/* 支持 IE */

	display: -webkit-box;

	display: -webkit-flex;

	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.textView {
	text-align: left;
	margin-left: 30rpx;
	font-size: 28rpx;
	/*超出一行点点点*/
	overflow: hidden;
	/*超出的部分隐藏起来。*/
	white-space: nowrap;
	/*不显示的地方用省略号...代替*/
	text-overflow: ellipsis;
	/* 支持 IE */
}
.mustView {
	color: red;
	font-size: 32rpx;
	margin-left: 10rpx;
}
.detailView1 {
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
	text-align: right;
	font-size: 28rpx;
	overflow: hidden;
	/*超出的部分隐藏起来。*/
	white-space: nowrap;
	/*不显示的地方用省略号...代替*/
	text-overflow: ellipsis;
	/* 支持 IE */
}
.detailView2 {
	width: calc(100% - 48rpx);
	text-align: left;
	font-size: 28rpx;
	word-wrap: break-word;
	word-break: normal;
	margin-top: 12rpx;
}
.bottomLine {
	width: 95%;
	height: 1rpx;
	background-color: #DDDDDD;
	margin-top: 30rpx;
}
.icon {
	width: 38rpx;
	height: 38rpx;
	margin-right: 10rpx;
}

